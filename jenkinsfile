pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building started by abhishek tiwari'
            }
        }
        stage('Test') {
            steps {
                echo 'testing' 
            }
        }
        stage('Deploy') {
            steps {
               sh 'docker build . -t ubuntu/abhi'
                docker 'run -p 3000:3000 -d ubuntu/abhi'
                docker 'run -p 3001:3001 -d ubuntu/abhi'
            }
        }
    }
}
